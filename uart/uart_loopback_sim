    %end;
t_2 ;
    %wait E_0x64b20cc3ab20;
    %load/vec4 v0x64b20cc61b20_0;
    %load/vec4 v0x64b20cc5ec90_0;
    %cmp/ne;
    %jmp/0xz  T_1.2, 6;
    %vpi_call 2 92 "$display", "    [FAIL] RX Mismatch! Sent %h, Got %h", v0x64b20cc5ec90_0, v0x64b20cc61b20_0 {0 0 0};
    %jmp T_1.3;
