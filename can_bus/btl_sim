#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b3ba95e52d0 .scope module, "tb_can_btl" "tb_can_btl" 2 3;
 .timescale -9 -12;
v0x5b3ba95fd390_0 .var "clk", 0 0;
v0x5b3ba95fd450_0 .var "rst", 0 0;
v0x5b3ba95fd520_0 .var "rx_sync_edge", 0 0;
v0x5b3ba95fd620_0 .net "sample_point", 0 0, v0x5b3ba95fd040_0;  1 drivers
v0x5b3ba95fd6f0_0 .net "tx_point", 0 0, v0x5b3ba95fd230_0;  1 drivers
S_0x5b3ba95e5460 .scope module, "uut" "can_btl" 2 17, 3 3 0, S_0x5b3ba95e52d0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "rx_sync_edge";
    .port_info 3 /OUTPUT 1 "sample_point";
    .port_info 4 /OUTPUT 1 "tx_point";
P_0x5b3ba95e5640 .param/l "BRP" 0 3 4, +C4<00000000000000000000000000000100>;
P_0x5b3ba95e5680 .param/l "TSEG1" 0 3 5, +C4<00000000000000000000000000001011>;
P_0x5b3ba95e56c0 .param/l "TSEG2" 0 3 6, +C4<00000000000000000000000000000100>;
v0x5b3ba95d1f50_0 .net "clk", 0 0, v0x5b3ba95fd390_0;  1 drivers
v0x5b3ba95d2810_0 .var "clk_count", 15 0;
v0x5b3ba95fceb0_0 .net "rst", 0 0, v0x5b3ba95fd450_0;  1 drivers
v0x5b3ba95fcf80_0 .net "rx_sync_edge", 0 0, v0x5b3ba95fd520_0;  1 drivers
v0x5b3ba95fd040_0 .var "sample_point", 0 0;
v0x5b3ba95fd150_0 .var "tq_count", 15 0;
v0x5b3ba95fd230_0 .var "tx_point", 0 0;
E_0x5b3ba95a7fb0 .event posedge, v0x5b3ba95d1f50_0;
    .scope S_0x5b3ba95e5460;
T_0 ;
    %wait E_0x5b3ba95a7fb0;
    %load/vec4 v0x5b3ba95fceb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95d2810_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95fd150_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b3ba95fd040_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b3ba95fd230_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b3ba95fd040_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5b3ba95fd230_0, 0;
    %load/vec4 v0x5b3ba95fcf80_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95d2810_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95fd150_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5b3ba95d2810_0;
    %pad/u 32;
    %cmpi/e 3, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95d2810_0, 0;
    %load/vec4 v0x5b3ba95fd150_0;
    %pad/u 32;
    %cmpi/e 14, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5b3ba95fd150_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5b3ba95fd230_0, 0;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x5b3ba95fd150_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x5b3ba95fd150_0, 0;
T_0.7 ;
    %load/vec4 v0x5b3ba95fd150_0;
    %pad/u 32;
    %cmpi/e 10, 0, 32;
    %jmp/0xz  T_0.8, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5b3ba95fd040_0, 0;
T_0.8 ;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5b3ba95d2810_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x5b3ba95d2810_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b3ba95e52d0;
T_1 ;
    %delay 10000, 0;
    %load/vec4 v0x5b3ba95fd390_0;
    %inv;
    %store/vec4 v0x5b3ba95fd390_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5b3ba95e52d0;
T_2 ;
    %vpi_call 2 31 "$dumpfile", "can_btl_wave.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b3ba95e52d0 {0 0 0};
    %vpi_call 2 35 "$display", "--- Starting CAN BTL Simulation ---" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b3ba95fd390_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b3ba95fd450_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b3ba95fd520_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b3ba95fd450_0, 0, 1;
    %vpi_call 2 42 "$display", "Time: %0t | Reset released. Normal operation begins.", $time {0 0 0};
    %delay 3000000, 0;
    %vpi_call 2 49 "$display", "Time: %0t | Firing rx_sync_edge (Hard Sync)!", $time {0 0 0};
    %wait E_0x5b3ba95a7fb0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b3ba95fd520_0, 0, 1;
    %wait E_0x5b3ba95a7fb0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b3ba95fd520_0, 0, 1;
    %delay 2000000, 0;
    %vpi_call 2 59 "$display", "--- Simulation Complete ---" {0 0 0};
    %vpi_call 2 60 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_can_btl.v";
